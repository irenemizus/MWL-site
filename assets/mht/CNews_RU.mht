    background: url("http://www.armd.ru/common/img/uploaded/armada_img/zag_bg_say_more.png") no-repeat scroll 15px 11px transparent;
    color: #505050;
    font: 12px Arial;
    padding: 10px 0 10px 30px;
    position: relative;
}
.div_say_more .as_in .x_say_more {
